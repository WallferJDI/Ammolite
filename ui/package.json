{
  "name": "knits-template",
  "version": "0.1.0",
  "engines": {
    "node": "16.14.2"
  },
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "install:clean": "rm -rf node_modules/ && npm ci && npm start",
    "json-server": "json-server --watch ./json-server-mock-data/db.js --port 8999 --delay 1000",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx ./src --ignore-path .gitignore",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "prepare": "cd .. && husky install ui/.husky"
  },
  "resolutions": {
    "vite/esbuild": "0.6.28"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.1.1",
    "@types/react-datepicker": "^4.4.2",
    "react": "^18.2.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "6.3.0",
    "react-select": "^5.3.2",
    "react-table": "^7.7.0",
    "reactstrap": "^9.1.1",
    "simplebar-react": "^2.4.1",
    "sweetalert2": "^11.4.24"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.2.1",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-table": "^7.7.12",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-react": "^1.3.2",
    "c8": "^7.11.3",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.1.1",
    "eslint-plugin-css-import-order": "^1.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "json-server": "^0.17.0",
    "msw": "^0.44.2",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vite-plugin-checker": "^0.4.6",
    "vite-plugin-pwa": "^0.12.0",
    "vitest": "^0.16.0"
  }
}
